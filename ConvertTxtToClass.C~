void ConvertTxtToClass(){
  int run = 110;
  int lumi = 1200;
  int event = 222230008;

  TString rle ;
  rle.Form("%d:%d:%d",run,lumi,event);
  std::cout<<" rle = "<<rle<<std::endl;
  std::ifstream fin("csc2015.txt");
  //std::ifstream fin("try.C");
  ofstream fout;
  fout.open("EventsToSkip.h");
  fout<<"#ifndef EventsToSkip_h_"<<std::endl;
  fout<<"#define EventsToSkip_h_ "<<std::endl;
  fout<<"#include \"TString.h\" "<<std::endl;
  fout<<"using namespace std; "<<std::endl;
  fout<<"class EventsToSkip{ "<<std::endl;
  fout<<" public: "<<std::endl;
  fout<<"   std::vector<TString> eventlist;"<<std::endl;
  fout<<"   EventsToSkip(){"<<std::endl;
  fout<<"   eventlist.clear();"<<std::endl;
  
  //fout<<"   static TString MakeAllEventList (){"<<std::endl;
  
  
    
  std::string line;
  if (fin.is_open())  {
    while (fin.good())	{
      getline(fin,line);
      fout<<"eventlist.push_back(\""<<line<<"\");"<<std::endl;
    }
    fin.close();
  }
  fout<<"  }"<<std::endl;
  fout<<"   ~EventsToSkip(){};"<<std::endl;
  fout<<"std::vector<TString> EventList(){"<<std::endl;
  fout<<"  return eventlist;"<<std::endl;
  fout<<"  }"<<std::endl;
  
  fout<<"};"<<std::endl;
  fout<<"#endif"<<std::endl;
  fout.close();


  else    {
      std::cout << "Unable to open file" << std::endl << std::endl;
    }

  
}
