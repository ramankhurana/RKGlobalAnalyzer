void temptreemaker(){
  TFile* f = new TFile("tryfile.root","RECREATE");

  std::vector<float> p2;
  std::vector<float> p3;
  p2.clear();
  p3.clear();

  TTree* tree = new TTree("t1","analtree");
  tree->Branch("p2","std::vector<float>", &p2);
  tree->Branch("p3","std::vector<float>", &p3);
  for (int i=0; i<2000; i++){
    p2.push_back(i*2.);
    p3.push_back(i*3.);
    tree->Fill();
  }
  f->cd();
  tree->Write();
}
